      ]
    },
    {
      "name": "arg is a function expression",
      "selector": "$.values[?length(@.a)==length(value($..c))]",
      "document": {
        "c": "cd",
        "values": [
          {
            "a": "ab"
          },
          {
            "a": "d"
          }
        ]
      },
      "result": [
        {
          "a": "ab"
        }
      ],
      "tags": [
        "function",
        "length"
      ],
      "result_paths": [
        "$['values'][0]"
      ]
    },
    {
      "name": "arg is special nothing",
      "selector": "$[?length(value(@.a))>0]",
      "document": [
        {
          "a": "ab"
        },
        {
          "c": "d"
        },
        {
          "a": null
        }
      ],
      "result": [

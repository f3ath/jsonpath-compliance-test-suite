{
  "tests": [
    {
      "name": "string data",
      "selector": "$[?length(@.a)>=2]",
      "document": [
        {
          "a": "ab"
        },
        {
          "a": "d"
        }
      ],
      "result": [
        {
          "a": "ab"
        }
      ],
      "tags": [
        "function",
        "length"
      ],
      "result_paths": [
        "$[0]"
      ]
    },
    {
      "name": "string data, unicode",
      "selector": "$[?length(@)==2]",
      "document": [
        "☺",
        "☺☺",
        "☺☺☺",
        "ж",
        "жж",
        "жжж",
        "磨",
        "阿美",
        "形声字"
      ],
      "result": [
        "☺☺",
        "жж",
        "阿美"
      ],
      "tags": [
        "function",
        "length"
      ],
      "result_paths": [
        "$[1]",
        "$[4]",
        "$[7]"
      ]
    },
    {
      "name": "array data",
      "selector": "$[?length(@.a)>=2]",
      "document": [
        {
          "a": [
            1,
            2,
            3
          ]
        },
        {
          "a": [
            1
          ]
        }
      ],
      "result": [
        {
          "a": [
            1,
            2,
            3
          ]
        }
      ],
      "tags": [
        "function",
        "length"
      ],
      "result_paths": [
        "$[0]"
      ]
    },
